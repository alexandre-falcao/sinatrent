<div class="container">
  <div class="row justify-content-center">

    <div class="form-login">
      <%= simple_form_for(instrument) do |f| %>
        <%= f.input :name %>
        <%= f.association :category %>
        <%= f.input :street %>
        <%= f.input :number %>
        <%= f.input :district %>
        <%= f.input :city %>
        <%= f.input :state %>
        <%= f.input :description %>
        <%= f.input :price %>
        <%= f.input :photo, as: :file %>
        <%= f.submit %>
      <% end %>
    </div>

  </div>
</div>
